{
  "name": "ic-vue-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "clean": "yarn backend:stop;",
    "backend:generate:bindings": "dfx generate data_manager",
    "backend:start": "yarn backend:up",
    "backend:up": "yarn clean; dfx start --background --clean;DFX_NETOWRK=local dfx deploy --network local",
    "backend:wipe": "dfx deploy --network local --mode reinstall data_manager",
    "backend:stop": "yarn backend:down",
    "backend:down": "dfx stop;rm -r .dfx/*/canisters || true;rm -r dist || true",
    "postinstall": "if [ -n \"${HUSKY}\" ] && [ \"${HUSKY}\" -eq 0 ]; then echo 'INFO: skip husky for ci/cd' ; else husky install; fi",
    "git:pre-push": ".husky/pre-push",
    "storybook:up": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "code:style": "yarn code:style:root && yarn code:style:frontend",
    "code:style:root": "yarn pprettier --ignore-path=.eslintignore --list-different \"*.{js,ts,tsx,json,css,vue,md,yml,yaml}\"",
    "code:style:frontend": "yarn pprettier --ignore-path=.eslintignore --list-different \"{src/frontend,e2e,cypress,.storybook}/**/*.{js,ts,tsx,json,css,vue,md,yml,yaml}\"",
    "code:style:fix": "yarn code:style:fix:root && yarn code:style:fix:frontend",
    "code:style:fix:root": "yarn pprettier --ignore-path=.eslintignore --write \"*.{js,ts,tsx,json,css,vue,md,yml,yaml}\"",
    "code:style:fix:frontend": "yarn pprettier --ignore-path=.eslintignore --write \"{src/frontend,e2e,cypress,.storybook}/**/*.{js,ts,tsx,json,css,vue,md,yml,yaml}\"",
    "code:analyse": "eslint  --ext .js --ext .vue --ext .yaml --ext .yml --ext .ts --ignore-path .eslintignore .",
    "code:analyse:fix": "eslint --ext .js --ext .vue --ext .yaml --ext .yml --ext .ts --ignore-path .eslintignore --fix .",
    "code:quality-suite": "yarn git:pre-push",
    "code:test:e2e": "start-server-and-test dev http-get://localhost:3000 'yarn code:test:e2e:run'",
    "code:test:e2e:open": "yarn cypress open",
    "code:test:e2e:run": "yarn cypress run",
    "code:test:unit": "vitest --dir src/frontend --run --reporter=junit --outputFile=vitest-test-result.xml; if [ -d src/frontend/node_modules ]; then rm -rf src/frontend/node_modules; fi",
    "code:test:unit:watch": "vitest --watch --reporter=dot --reporter=json --outputFile=../../.vitest-result.json",
    "code:test:storybook": "yarn concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook\"",
    "code:test": "yarn code:test:unit && yarn code:test:storybook && yarn code:test:e2e"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/types": "^7.18.4",
    "@faker-js/faker": "^7.2.0",
    "@mixer/parallel-prettier": "^2.0.3",
    "@pinia/testing": "^0.0.12",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.38",
    "@storybook/test-runner": "^0.3.0",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.9",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitest/ui": "^0.18.1",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/test-utils": "^2.0.0-rc.18",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.5",
    "buffer": "^6.0.3",
    "caniuse-lite": "^1.0.30001349",
    "cypress": "^10.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.13",
    "eslint-plugin-vue": "^7.20.0",
    "eslint-plugin-yaml": "^0.5.0",
    "happy-dom": "^6.0.3",
    "husky": "^7.0.4",
    "jest": "27",
    "lodash": "^4.17.21",
    "postcss": "^8.4.14",
    "postcss-import": "^14.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.3",
    "vite": "^3.0.0",
    "vitest": "^0.18.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.37.1"
  },
  "dependencies": {
    "@dfinity/agent": "^0.11.3",
    "@dfinity/auth-client": "^0.11.3",
    "@dfinity/authentication": "^0.11.3",
    "@dfinity/candid": "^0.11.3",
    "@dfinity/identity": "^0.11.3",
    "@dfinity/principal": "^0.11.3",
    "@headlessui/vue": "^1.6.7",
    "@heroicons/vue": "^1.0.6",
    "@vueuse/core": "^8.6.0",
    "concurrently": "^7.2.2",
    "json-to-csv-export": "^1.1.1",
    "json2csv": "^5.0.7",
    "papaparse": "^5.3.2",
    "pinia": "^2.0.14",
    "ufostart-flightplan-api-client": "^0.0.11",
    "uuid": "^8.3.2",
    "vue": "^3.2.37",
    "vue-router": "^4.0.13",
    "vuedraggable": "^4.1.0"
  }
}
